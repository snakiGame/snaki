{
  "expo": {
    "name": "snaki",
    "slug": "snaki",
    "version": "1.5.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "snaki",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tach.snaki",
       "icon": "./assets/icon/ios/AppIcon~ios-marketing.png"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/android/res/mipmap-xxxhdpi/ic_launcher_foreground.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.tach.snaki"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-sqlite",
      "expo-web-browser",
      "expo-splash-screen"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4b0709bf-3f8d-4bad-9b69-afba24f8040a"
      }
    }
  }
}
